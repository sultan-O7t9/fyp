{
  "name": "pmo-management-system",
  "version": "1.0.0",
  "description": "PMO Management System is a FYP project for the University of Gujrat to develop a web application for managing PMO activities.",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
